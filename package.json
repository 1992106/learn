{
  "name": "learn",
  "version": "1.0.0",
  "description": "cli and Some utility functions(ts/js)",
  "scripts": {
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:es": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:style": "stylelint src/**/*.less --syntax less --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1992106/learn.git"
  },
  "keywords": [
    "cli",
    "utils",
    "js",
    "jsx",
    "ts",
    "tsx",
    "vue",
    "react",
    "angular"
  ],
  "author": "王会俊",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1992106/learn/issues"
  },
  "homepage": "https://github.com/1992106/learn#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^14.0.27",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^3.9.5"
  },
  "lint-staged": {
    "src/**/*{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.less": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.5.0",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "vue": "^2.6.12"
  }
}
